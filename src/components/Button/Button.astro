---
import "./Button.scss";
import cx from "classnames";

export interface Props {
	href: string,
	isPrimary?: boolean,
}
const { href, isPrimary } = Astro.props;
---

<a
	class={cx("Button anim_outline", isPrimary && "_primary")}
	href={href}
	{/* target="_blank"
	rel="noreferrer" */}
>
	<slot />
</a>